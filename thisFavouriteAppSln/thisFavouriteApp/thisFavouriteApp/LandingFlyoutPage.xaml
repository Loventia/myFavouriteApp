<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="thisFavouriteApp.LandingFlyoutPage"
             xmlns:pages="clr-namespace:thisFavouriteApp">
  <FlyoutPage.Flyout>
        <pages:LandingFlyoutPageFlyout x:Name="FlyoutPage" BackgroundColor="Black" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail >
        <NavigationPage BarBackgroundColor="#1DB954">
      <x:Arguments>
        <pages:MainPage />
      </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>